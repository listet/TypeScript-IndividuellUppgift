{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "outDir": "./dist",
        "strict": true, //För striktare typ-kontroll
        "noUnusedLocals": true, //varnar om variabler är deklarerade men inte används
        // "noImplicitAny": true  / ingår i strict
        "forceConsistentCasingInFileNames": true //säkerställer att filnamn är konsekventa utifrån stora och små bokstäver
    },
    "include": [
        "./src"
    ],
    "exclude": []
}